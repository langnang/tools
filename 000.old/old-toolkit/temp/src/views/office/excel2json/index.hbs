<style>
  caption{ text-align: center; font-size: 20px; }
</style>
<div class="container">
  {{>page-header}}
  <div class="row">
    <div class="col-md-6">
      <div class="form-group">
        <input type="file" id="input_file" accept="application/vnd.ms-excel,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />
      </div>
    </div>
    <div class="col-md-6">
      <select class="form-control" name="sheet_names">
      </select>
    </div>
    <div class="col-md-12">
      <table class="table table-striped table-bordered table-hover table-condensed" name="sheet_table">
      </table>
    </div>
  </div>
</div>
